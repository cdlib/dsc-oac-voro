            <dc:title>Boat House and Lake at Lucky Baldwin's Ranch, Santa Anita, Cal.</dc:title>
            <dc:creator>unknown</dc:creator>
            <dc:date>circa 1893</dc:date>
            <dc:subject>Ranches--California</dc:subject>
            <dc:type>image</dc:type>
            <dc:type>Photographs</dc:type>
